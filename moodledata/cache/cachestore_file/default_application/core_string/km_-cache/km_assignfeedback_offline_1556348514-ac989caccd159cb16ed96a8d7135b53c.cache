a:25:{s:13:"confirmimport";s:66:"អះអាងពីការនាំចូលពិន្ទុ";s:7:"default";s:30:"បើកជាស្រេច";s:12:"default_help";s:319:"ប្រសិនបើបើក នោះការដាក់ពិន្ទុក្រៅបណ្តាញដោយប្រើសន្លឹកកិច្ចការនឹងត្រូវបានបើកជាស្រេចសម្រាប់កិច្ចការថ្មីទាំងអស់។";s:14:"downloadgrades";s:108:"ទាញយកសន្លឹកកិច្ចការសម្រាប់ដាក់ពិន្ទុ";s:7:"enabled";s:123:"សន្លឹកកិច្ចការសម្រាប់ដាក់ពិន្ទុក្រៅបណ្តាញ";s:12:"enabled_help";s:280:"ប្រសិនបើបើក គ្រូនឹងអាចទាញយកនិងផ្ទុកឡើងសន្លឹិកកិច្ចការដែលមានពិន្ទុសិស្សនៅពេលដាកើពិន្ទុកិច្ចការ។";s:14:"feedbackupdate";s:132:"ដាក់ប្រអប់ទិន្នន័យ "{$a->field}" សម្រាប់ "{$a->student}" ទៅជា "{$a->text}"";s:21:"graderecentlymodified";s:242:"ពិន្ទុត្រូវបានកែប្រែក្នុង Moodle ថ្មីជាងពិន្ទុនៅក្នុងសន្លឹកកិច្ចការដាក់ពិន្ទុសម្រាប់  {$a}";s:22:"gradelockedingradebook";s:134:"ពិន្ទុត្រូវបានចាក់សោក្នុងបញ្ជីពិន្ទុសម្រាប់ {$a}";s:11:"gradeupdate";s:90:"ដាក់ពិន្ទុសម្រាប់ {$a->student} ទៅជា {$a->grade}";s:14:"ignoremodified";s:287:"អនុញ្ញាតឱ្យកែប្រែជាថ្មីនូវកំណត់ត្រាដែលត្រូវបានកែប្រែនៅក្នុង Moodle ក្នុងពេលថ្មីៗជាងនៅក្នុងសៀវភៅបញ្ជី។";s:19:"ignoremodified_help";s:425:"When the grading worksheet is downloaded from Moodle it contains the last modified date for each of the grades. If any of the grades are updated in Moodle after this worksheet is downloaded, by default Moodle will refuse to overwrite this updated information when importing the grades. By selecting this option Moodle will disable this safety check and it may be possible for multiple markers to overwrite each others grades.";s:12:"importgrades";s:129:"អះអាងការកែប្រែក្នុងសន្លឹកកិច្ចការដាក់ពិន្ទុ";s:18:"invalidgradeimport";s:352:"Moodle មិនអាចអានសន្លឹកកិច្ចការដែលបានផ្ទុកឡើងទេ។ សូមធ្វើឲ្យប្រាកដថាវាត្រូវបានរក្សាទុកក្នុងទម្រង់ឯកសារ CSV ហើយសូមព្យាយាមម្តងទៀត។";s:10:"gradesfile";s:97:"សន្លឹកកិច្ចការដាក់ពិន្ទុ (ប្រភេទ csv)";s:15:"gradesfile_help";s:603:"សន្លឹិកកិច្ចការដាក់ពិន្ទុដែលពិន្ទុត្រូវបានកែប្រែ។ ឯកសារនេះត្រូវជាឯកសារ CSV ដែលត្រូវបានទាញយកពីកិច្ចការនេះ និងត្រូវមានជួរឈរសម្រាប់ពិន្ទុសិស្ស និង ព័ត៌មានអត្តសញ្ញាណ។ កម្មវិធីបម្លែងកូដសម្រាប់ឯកសារនេះត្រូវតែជា "UFT-8"។";s:26:"privacy:nullproviderreason";s:133:"This plugin has no database to store user information. It only uses APIs in mod_assign to help with displaying the grading interface.";s:9:"nochanges";s:183:"រកមិនឃើញពិន្ទុដែលបានកែប្រែក្នុងសន្លឹកកិច្ចការដែលបានផ្ទុកឡើងទេ";s:23:"offlinegradingworksheet";s:18:"ពិន្ទុ";s:10:"pluginname";s:123:"សន្លឹកកិច្ចការសម្រាប់ដាក់ពិន្ទុក្រៅបណ្តាញ";s:13:"processgrades";s:36:"នាំចូលពិន្ទុ";s:10:"skiprecord";s:39:"រំលងកំណត់ត្រា";s:12:"updaterecord";s:57:"កែប្រែកំណត់ត្រាថ្មី";s:12:"uploadgrades";s:105:"ផ្ទុកឡើងនូវសន្លឹកកិច្ចការដាក់ពិន្ទុ";s:13:"updatedgrades";s:108:"បានកែប្រែពិន្ទុ និងមូលវិចារណ៍ជាថ្មី {$a}";}